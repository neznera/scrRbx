-- Сканер объектов (выводит только объекты с "flake" в имени)
while task.wait(3) do
    print("=== Поиск объектов с 'flake' ===")
    local found = false
    for _, obj in pairs(workspace:GetDescendants()) do
        local name = obj.Name:lower()
        if name:find("flake") then  -- ищем только "flake"
            print("Найден объект:")
            print("  Имя:", obj.Name)
            print("  Класс:", obj.ClassName)
            print("  Путь:", obj:GetFullName())
            print("---")
            found = true
        end
    end
    if not found then
        print("Ничего с 'fl2323ake' не найдено.")
    end
end

-- Инспектор объектов (наведись на токен и смотри консоль)
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local LocalPlayer = Players.LocalPlayer

local mouse = LocalPlayer:GetMouse()

mouse.Move:Connect(function()
    local target = mouse.Target
    if target then
        -- Очищаем консоль (если нужно, можно закомментировать)
        print("-----")
        print("Имя:", target.Name)
        print("Класс:", target.ClassName)
        print("Путь:", target:GetFullName())
        print("Родитель:", target.Parent and target.Parent.Name or "nil")
    end
end)

print("✅ Инспектор запущен. Наводи мышку на токены и смотри консоль (F9).")

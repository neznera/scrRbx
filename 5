--[[
    АВТОМАТИЧЕСКАЯ ТЕЛЕПОРТАЦИЯ НА ТОКЕНЫ (ИМЯ "C")
    - Постоянно ищет ближайший объект с именем "C".
    - Мгновенно телепортирует персонажа прямо в центр объекта.
    - Работает без остановки, пока скрипт запущен.
    - Для остановки просто закройте игру или удалите скрипт.
]]

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- НАСТРОЙКИ
local TARGET_NAME = "C"                     -- имя токена (как у вас)
local TELEPORT_OFFSET = Vector3.new(0, 0, 0) -- можно добавить смещение, если надо

-- Функция поиска ближайшего объекта с нужным именем
local function getClosestTarget()
    local char = LocalPlayer.Character
    if not char then return nil end
    local root = char:FindFirstChild("HumanoidRootPart") or char:FindFirstChild("Torso")
    if not root then return nil end
    local myPos = root.Position

    local closestObj = nil
    local closestDist = math.huge

    -- Перебираем все объекты в workspace
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and obj.Name == TARGET_NAME then
            local dist = (obj.Position - myPos).Magnitude
            if dist < closestDist then
                closestDist = dist
                closestObj = obj
            end
        end
    end
    return closestObj
end

-- Главный цикл телепортации
while true do
    local target = getClosestTarget()
    if target then
        local char = LocalPlayer.Character
        if char then
            local root = char:FindFirstChild("HumanoidRootPart") or char:FindFirstChild("Torso")
            if root then
                -- Мгновенное перемещение в центр токена
                root.CFrame = CFrame.new(target.Position + TELEPORT_OFFSET)
            end
        end
    end
    -- Небольшая задержка, чтобы не нагружать процессор (можно убрать, но тогда нагрузка будет 100%)
    task.wait()
end

local found = {}
for _, obj in pairs(game:GetDescendants()) do
    if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
        table.insert(found, obj:GetFullName() .. " (" .. obj.ClassName .. ")")
    end
end
print("Всего Remote'ов: " .. #found)
for i, v in ipairs(found) do
    print(i .. ". " .. v)
end

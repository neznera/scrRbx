-- ВКЛАДКИ
local TabFrame = Instance.new("Frame")
TabFrame.Size = UDim2.new(1, 0, 0, 40)
TabFrame.Position = UDim2.new(0, 0, 0, 45)
TabFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
TabFrame.BorderSizePixel = 0
TabFrame.Parent = MainFrame

local Tabs = {"Главная", "Флинг/Кик", "ESP/Визуал", "Гравитация"}
local TabButtons = {}

local function SelectTab(tabName)
    Settings.CurrentTab = tabName
    for _, btn in pairs(TabButtons) do
        btn.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    end
    if TabButtons[tabName] then
        TabButtons[tabName].BackgroundColor3 = Color3.fromRGB(150, 0, 0)
    end
end

for i, name in ipairs(Tabs) do
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 140, 0, 30)
    btn.Position = UDim2.new(0, 10 + (i-1)*150, 0, 5)
    btn.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    btn.Text = name
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 16
    btn.Parent = TabFrame
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 6)
    btnCorner.Parent = btn
    
    TabButtons[name] = btn
    btn.MouseButton1Click:Connect(function() SelectTab(name) end)
end

-- КОНТЕНТ
local ContentFrame = Instance.new("ScrollingFrame")
ContentFrame.Size = UDim2.new(1, -20, 1, -120)
ContentFrame.Position = UDim2.new(0, 10, 0, 90)
ContentFrame.BackgroundTransparency = 1
ContentFrame.ScrollBarThickness = 8
ContentFrame.ScrollBarImageColor3 = Color3.fromRGB(150, 0, 0)
ContentFrame.CanvasSize = UDim2.new(0, 0, 2, 0)
ContentFrame.Parent = MainFrame

-- =====================================================================
-- БЛОК 11: ФУНКЦИИ ИНТЕРФЕЙСА
-- =====================================================================
local function CreateButton(parent, text, pos, callback)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 180, 0, 40)
    btn.Position = UDim2.new(0, pos.X, 0, pos.Y)
    btn.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
    btn.Text = text
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 16
    btn.Parent = parent
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 8)
    btnCorner.Parent = btn
    
    btn.MouseButton1Click:Connect(callback)
    return btn
end

local function CreateSlider(parent, text, pos, getter, setter, min, max, format)
    local bg = Instance.new("Frame")
    bg.Size = UDim2.new(0, 200, 0, 50)
    bg.Position = UDim2.new(0, pos.X, 0, pos.Y)
    bg.BackgroundTransparency = 1
    bg.Parent = parent
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 0, 20)
    label.BackgroundTransparency = 1
    label.Text = text .. ": " .. string.format(format, getter())
    label.TextColor3 = Color3.new(1,1,1)
    label.Font = Enum.Font.Gotham
    label.TextSize = 16
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Parent = bg
    
    local slider = Instance.new("Frame")
    slider.Size = UDim2.new(1, 0, 0, 8)
    slider.Position = UDim2.new(0, 0, 0, 30)
    slider.BackgroundColor3 = Color3.fromRGB(60,60,60)
    slider.Parent = bg
    
    local fill = Instance.new("Frame")
    fill.Size = UDim2.new((getter() - min) / (max - min), 0, 1, 0)
    fill.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    fill.Parent = slider
    
    local dragging = false
    fill.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
        end
    end)
    
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    RunService.RenderStepped:Connect(function()
        if dragging then
            local mousePos = UserInputService:GetMouseLocation()
            local absX = slider.AbsolutePosition.X
            local absSizeX = slider.AbsoluteSize.X
            local relX = math.clamp(mousePos.X - absX, 0, absSizeX)
            local percent = relX / absSizeX
            local val = min + (max - min) * percent
            setter(val)
            fill.Size = UDim2.new((val - min) / (max - min), 0, 1, 0)
            label.Text = text .. ": " .. string.format(format, val)
        end
    end)
end

local function CreatePlayerList(parent, pos)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 200, 0, 300)
    frame.Position = UDim2.new(0, pos.X, 0, pos.Y)
    frame.BackgroundColor3 = Color3.fromRGB(30,30,40)
    frame.Parent = parent
    
    local list = Instance.new("ScrollingFrame")
    list.Size = UDim2.new(1, -10, 1, -10)
    list.Position = UDim2.new(0, 5, 0, 5)
    list.BackgroundTransparency = 1
    list.ScrollBarThickness = 6
    list.CanvasSize = UDim2.new(0,0,2,0)
    list.Parent = frame
    
    local updateList
    updateList = function()
        for _, v in pairs(list:GetChildren()) do
            if v:IsA("TextButton") then v:Destroy() end
        end
        
        local y = 0
        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer then
                local btn = Instance.new("TextButton")
                btn.Size = UDim2.new(1, -10, 0, 35)
                btn.Position = UDim2.new(0, 5, 0, y)
                btn.BackgroundColor3 = Color3.fromRGB(50,50,70)
                btn.Text = p.Name
                btn.TextColor3 = Color3.new(1,1,1)
                btn.Font = Enum.Font.Gotham
                btn.TextSize = 16
                btn.Parent = list
                
                btn.MouseButton1Click:Connect(function()
                    Settings.FlingTarget = p
                end)
                
                y = y + 40
            end
        end
        list.CanvasSize = UDim2.new(0,0,0, y + 10)
    end
    
    Players.PlayerAdded:Connect(updateList)
    Players.PlayerRemoving:Connect(updateList)
    task.spawn(updateList)
    
    return frame
end

-- =====================================================================
-- БЛОК 12: РЕНДЕР КОНТЕНТА
-- =====================================================================
local function RenderContent()
    for _, v in pairs(ContentFrame:GetChildren()) do
        if v:IsA("Frame") or v:IsA("TextButton") or v:IsA("ScrollingFrame") then
            v:Destroy()
        end
    end
    
    if Settings.CurrentTab == "Главная" then
        CreateButton(ContentFrame, "🕊️ FLY: " .. (Settings.FlyEnabled and "ON" or "OFF"), Vector2.new(20, 20), ToggleFly)
        CreateSlider(ContentFrame, "Скорость полёта", Vector2.new(20, 80), 
            function() return Settings.FlySpeed end,
            function(v) Settings.FlySpeed = v end, 20, 200, "%.0f")
        
        CreateButton(ContentFrame, "⚡ ФЛИНГ ЦЕЛИ", Vector2.new(20, 150), function()
            if Settings.FlingTarget then
                StartFling(Settings.FlingTarget)
            end
        end)
        
        CreateSlider(ContentFrame, "Мощь флинга", Vector2.new(20, 210),
            function() return Settings.FlingPower end,
            function(v) Settings.FlingPower = v end, 1000, 20000, "%.0f")
        
        CreateButton(ContentFrame, "🛑 СТОП ФЛИНГ", Vector2.new(20, 280), StopFling)
        
    elseif Settings.CurrentTab == "Флинг/Кик" then
        CreatePlayerList(ContentFrame, Vector2.new(20, 20))
        
        CreateButton(ContentFrame, "💥 ФЛИНГ ВЫБРАННОГО", Vector2.new(250, 20), function()
            if Settings.FlingTarget then
                StartFling(Settings.FlingTarget)
            end
        end)
        
        CreateButton(ContentFrame, "👢 КИК ВЫБРАННОГО", Vector2.new(250, 80), function()
            if Settings.FlingTarget then
                KickPlayer(Settings.FlingTarget, "Изгнан EXILE HUB")
            end
        end)
        
        CreateButton(ContentFrame, "💀 КРАШ ВЫБРАННОГО", Vector2.new(250, 140), function()
            if Settings.FlingTarget then
                CrashPlayer(Settings.FlingTarget)
            end
        end)
        
    elseif Settings.CurrentTab == "ESP/Визуал" then
        CreateButton(ContentFrame, "ESP: " .. (Settings.EspEnabled and "ON" or "OFF"), Vector2.new(20, 20), function()
            Settings.EspEnabled = not Settings.EspEnabled
            if Settings.EspEnabled then RefreshESP() else
                for p,_ in pairs(ESP_Cache) do RemoveESP(p) end
            end
        end)
        
    elseif Settings.CurrentTab == "Гравитация" then
        CreateButton(ContentFrame, "🌍 ГРАВИТАЦИЯ: " .. (Settings.GravityEnabled and "КАБУМ" or "НОРМА"), Vector2.new(20, 20), ToggleGravity)
        CreateSlider(ContentFrame, "Сила гравитации", Vector2.new(20, 80),
            function() return Settings.GravityPower end,
            function(v) Settings.GravityPower = v; if Settings.GravityEnabled then Workspace.Gravity = v end end, 1, 500, "%.0f")
    end
end

SelectTab("Главная")
ContentFrame.ChildAdded:Connect(RenderContent)
ContentFrame.ChildRemoved:Connect(RenderContent)

-- =====================================================================
-- БЛОК 13: УПРАВЛЕНИЕ ПОЛЁТОМ
-- =====================================================================
RunService.Heartbeat:Connect(function()
    if Settings.FlyEnabled and LocalPlayer.Character then
        local root = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if root and Settings.FlyBody and Settings.FlyGyro then
            local move = Vector3.new()
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then move = move + Camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then move = move - Camera.CFrame.LookVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then move = move - Camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then move = move + Camera.CFrame.RightVector end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then move = move + Vector3.new(0,1,0) end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then move = move - Vector3.new(0,1,0) end
            
            if move.Magnitude > 0 then move = move.Unit * Settings.FlySpeed end
            Settings.FlyBody.Velocity = move
            Settings.FlyGyro.CFrame = CFrame.lookAt(root.Position, root.Position + Camera.CFrame.LookVector)
        end
    end
end)

-- =====================================================================
-- БЛОК 14: ГОРЯЧИЕ КЛАВИШИ
-- =====================================================================
UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.KeyCode == Enum.KeyCode.F then ToggleFly() end
    if input.KeyCode == Enum.KeyCode.G then ToggleGravity() end
end)

-- =====================================================================
-- БЛОК 15: ФИНАЛ
-- =====================================================================
RefreshESP()
RenderContent()

print([[

-- –ë–õ–û–ö 7: –û–ë–†–ê–ë–û–¢–ß–ò–ö–ò –°–û–ë–´–¢–ò–ô (–ö–õ–ò–ö–ò, –°–õ–ê–ô–î–ï–†–´, –ò–í–ï–ù–¢–´)
-- =====================================================================

-- 7.1: –ö–Ω–æ–ø–∫–∞ "–í—ã–±—Ä–∞—Ç—å –æ–±—ä–µ–∫—Ç"
SelectButton.MouseButton1Click:Connect(function()
    local target = Mouse.Target
    if target then
        Settings.SelectedObject = target
        UpdateSelectedInfo()
        UpdateHighlight()
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º GUI
        NameValue.Text = Settings.SelectedObjectName
        ClassValue.Text = Settings.SelectedObjectClass
        PathValue.Text = Settings.SelectedObjectPath
    else
        print("‚ö†Ô∏è –ù–µ—Ç –æ–±—ä–µ–∫—Ç–∞ –ø–æ–¥ –∫—É—Ä—Å–æ—Ä–æ–º")
    end
end)

-- 7.2: –ö–Ω–æ–ø–∫–∞ "–°–∫—Ä—ã—Ç—å" (—É–¥–∞–ª–∏—Ç—å —Å –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å—é –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è)
DeleteButton.MouseButton1Click:Connect(function()
    DeleteObject(false)
    NameValue.Text = Settings.SelectedObjectName
    ClassValue.Text = Settings.SelectedObjectClass
    PathValue.Text = Settings.SelectedObjectPath
end)

-- 7.3: –ö–Ω–æ–ø–∫–∞ "–£–Ω–∏—á—Ç–æ–∂–∏—Ç—å"
DestroyButton.MouseButton1Click:Connect(function()
    DeleteObject(true)
    NameValue.Text = Settings.SelectedObjectName
    ClassValue.Text = Settings.SelectedObjectClass
    PathValue.Text = Settings.SelectedObjectPath
end)

-- 7.4: –ö–Ω–æ–ø–∫–∞ "–í–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å"
RestoreButton.MouseButton1Click:Connect(function()
    RestoreObject()
    NameValue.Text = Settings.SelectedObjectName
    ClassValue.Text = Settings.SelectedObjectClass
    PathValue.Text = Settings.SelectedObjectPath
end)

-- 7.5: –ß–µ–∫–±–æ–∫—Å "–ü–æ–¥—Å–≤–µ—Ç–∫–∞"
HighlightEnabledCheckbox.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Settings.HighlightEnabled = not Settings.HighlightEnabled
        HighlightEnabledCheckbox.BackgroundColor3 = Settings.HighlightEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(80, 80, 80)
        UpdateHighlight()
    end
end)

-- 7.6: –ß–µ–∫–±–æ–∫—Å "–í–∏–¥–∏–º–æ –¥–ª—è –≤—Å–µ—Ö"
ForAllCheckbox.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Settings.HighlightForAll = not Settings.HighlightForAll
        ForAllCheckbox.BackgroundColor3 = Settings.HighlightForAll and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(80, 80, 80)
        UpdateHighlight()
    end
end)

-- 7.7: –°–ª–∞–π–¥–µ—Ä –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç–∏
local transpDragging = false
TranspFill.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        transpDragging = true
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        transpDragging = false
    end
end)

-- 7.8: –°–ª–∞–π–¥–µ—Ä —Å–∫–æ—Ä–æ—Å—Ç–∏
local speedDragging = false
SpeedFill.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        speedDragging = true
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        speedDragging = false
    end
end)

-- 7.9: –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–ª–∞–π–¥–µ—Ä–æ–≤ –≤ RenderStepped
RunService.RenderStepped:Connect(function()
    -- –°–ª–∞–π–¥–µ—Ä –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç–∏
    if transpDragging then
        local mousePos = UserInputService:GetMouseLocation()
        local absX, absY = TranspSlider.AbsolutePosition.X, TranspSlider.AbsolutePosition.Y
        local absSizeX = TranspSlider.AbsoluteSize.X
        local relativeX = math.clamp(mousePos.X - absX, 0, absSizeX)
        local percent = relativeX / absSizeX
        Settings.HighlightTransparency = math.clamp(percent, 0, 1)
        TranspFill.Size = UDim2.new(Settings.HighlightTransparency, 0, 1, 0)
        TranspLabel.Text = "–ü—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å: " .. string.format("%.1f", Settings.HighlightTransparency)
        UpdateHighlight()
    end
    
    -- –°–ª–∞–π–¥–µ—Ä —Å–∫–æ—Ä–æ—Å—Ç–∏
    if speedDragging then
        local mousePos = UserInputService:GetMouseLocation()
        local absX, absY = SpeedSlider.AbsolutePosition.X, SpeedSlider.AbsolutePosition.Y
        local absSizeX = SpeedSlider.AbsoluteSize.X
        local relativeX = math.clamp(mousePos.X - absX, 0, absSizeX)
        local percent = relativeX / absSizeX
        local value = math.floor(1 + (19 * percent))
        Settings.MoveSpeed = math.clamp(value, 1, 20)
        SpeedFill.Size = UDim2.new((Settings.MoveSpeed - 1) / 19, 0, 1, 0)
        SpeedLabel.Text = "–°–∫–æ—Ä–æ—Å—Ç—å: " .. Settings.MoveSpeed
    end
    
    -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ–± –æ–±—ä–µ–∫—Ç–µ –≤ —Ä–µ–∞–ª—å–Ω–æ–º –≤—Ä–µ–º–µ–Ω–∏
    UpdateSelectedInfo()
    NameValue.Text = Settings.SelectedObjectName
    ClassValue.Text = Settings.SelectedObjectClass
    PathValue.Text = Settings.SelectedObjectPath
end)

-- =====================================================================
-- –ë–õ–û–ö 8: –£–ü–†–ê–í–õ–ï–ù–ò–ï –° –ö–õ–ê–í–ò–ê–¢–£–†–´ (–ü–ï–†–ï–ú–ï–©–ï–ù–ò–ï –û–ë–™–ï–ö–¢–ê)
-- =====================================================================
UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    
    -- –ü–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ: WASD + Space/Ctrl
    if input.KeyCode == Enum.KeyCode.W then
        MoveObject(Camera.CFrame.LookVector)
    elseif input.KeyCode == Enum.KeyCode.S then
        MoveObject(-Camera.CFrame.LookVector)
    elseif input.KeyCode == Enum.KeyCode.A then
        MoveObject(-Camera.CFrame.RightVector)
    elseif input.KeyCode == Enum.KeyCode.D then
        MoveObject(Camera.CFrame.RightVector)
    elseif input.KeyCode == Enum.KeyCode.Space then
        MoveObject(Vector3.new(0, 1, 0))
    elseif input.KeyCode == Enum.KeyCode.LeftControl then
        MoveObject(Vector3.new(0, -1, 0))
    end
    
    -- –ü–æ–≤–æ—Ä–æ—Ç: Q/E (Yaw), Z/C (Pitch)
    if input.KeyCode == Enum.KeyCode.Q then
        RotateObject("Y", 15)
    elseif input.KeyCode == Enum.KeyCode.E then
        RotateObject("Y", -15)
    elseif input.KeyCode == Enum.KeyCode.Z then
        RotateObject("X", 15)
    elseif input.KeyCode == Enum.KeyCode.C then
        RotateObject("X", -15)
    end
end)

-- =====================================================================
-- –ë–õ–û–ö 9: –§–ò–ù–ê–õ–¨–ù–ê–Ø –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø
-- =====================================================================
print([[

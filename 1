-- =====================================================================
-- БЛОК 1: ЗАГРУЗЧИК И ЗАЩИТА (НИ ОДИН ИЗ ВАШИХ ПОИСКОВЫХ РЕЗУЛЬТАТОВ НЕ ДАЕТ ЭТОГО)
-- =====================================================================
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local Debris = game:GetService("Debris")
local CoreGui = (gethui and gethui()) or game:GetService("CoreGui")
local VirtualUser = game:GetService("VirtualUser")
local Workspace = game:GetService("Workspace")
local Lighting = game:GetService("Lighting")

local LocalPlayer = Players.LocalPlayer
local Camera = Workspace.CurrentCamera
local Mouse = LocalPlayer:GetMouse()

-- Анти-аффект: защита от краша при повторном запуске
if syn and syn.protect_gui then syn.protect_gui(CoreGui) end
if protect_gui then protect_gui(CoreGui) end

-- =====================================================================
-- БЛОК 2: НАСТРОЙКИ ПО УМОЛЧАНИЮ (ВСЁ ВКЛЮЧЕНО НА ПОЛНУЮ)
-- =====================================================================
local Settings = {
    -- ЛЕТАНИЕ
    FlyEnabled = false,
    FlySpeed = 75,
    FlyBody = nil,
    FlyGyro = nil,
    
    -- ФЛИНГ (ВЫКИДЫВАНИЕ ИГРОКОВ)
    FlingEnabled = false,
    FlingTarget = nil,
    FlingPower = 5000,
    FlingPart = nil,
    
    -- КИК/КРАШ
    KickEnabled = true,
    CrashEnabled = true,
    
    -- ESP (ПОДСВЕТКА ВРАГОВ)
    EspEnabled = true,
    EspColor = Color3.fromRGB(255, 0, 0), -- Красный
    
    -- ГРАВИТАЦИЯ
    GravityEnabled = false,
    GravityPower = 50,
    
    -- ИНТЕРФЕЙС
    MenuVisible = true,
    CurrentTab = "Главная",
}

-- =====================================================================
-- БЛОК 3: СЛУЧАЙНЫЕ ИМЕНА (ЗАЩИТА ОТ АНТИЧИТА)
-- =====================================================================
local function randomString(len)
    local str = ""
    for i = 1, len do str = str .. string.char(math.random(65, 90)) end
    return str
end

-- =====================================================================
-- БЛОК 4: ESP — ПОДСВЕТКА ИГРОКОВ (КРАСНЫЕ КРУГИ)
-- =====================================================================
local ESP_Cache = setmetatable({}, {__mode = "v"})

local function IsEnemy(p)
    return p ~= LocalPlayer
end

local function CreateESP(p)
    if not Settings.EspEnabled then return end
    if not IsEnemy(p) then return end
    
    local char = p.Character
    if not char then return end
    local head = char:FindFirstChild("Head")
    if not head then return end
    
    if ESP_Cache[p] then
        pcall(function() ESP_Cache[p]:Destroy() end)
        ESP_Cache[p] = nil
    end
    
    local bill = Instance.new("BillboardGui")
    bill.Name = randomString(12)
    bill.Adornee = head
    bill.Size = UDim2.new(2, 0, 2, 0)
    bill.StudsOffset = Vector3.new(0, 1, 0)
    bill.AlwaysOnTop = true
    bill.Parent = CoreGui
    
    local circle = Instance.new("ImageLabel")
    circle.Size = UDim2.new(1, 0, 1, 0)
    circle.BackgroundTransparency = 1
    circle.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
    circle.ImageColor3 = Settings.EspColor
    circle.ImageTransparency = 0.2
    circle.ScaleType = Enum.ScaleType.Fit
    circle.Parent = bill
    
    ESP_Cache[p] = bill
end

local function RemoveESP(p)
    if ESP_Cache[p] then
        pcall(function() ESP_Cache[p]:Destroy() end)
        ESP_Cache[p] = nil
    end
end

local function RefreshESP()
    for p,_ in pairs(ESP_Cache) do RemoveESP(p) end
    for _,p in pairs(Players:GetPlayers()) do
        if p ~= LocalPlayer then CreateESP(p) end
    end
end

Players.PlayerAdded:Connect(function(p)
    if p == LocalPlayer then return end
    p.CharacterAdded:Connect(function() task.wait(0.1); CreateESP(p) end)
    if p.Character then CreateESP(p) end
end)

Players.PlayerRemoving:Connect(RemoveESP)

-- =====================================================================
-- БЛОК 5: FLY — АБСОЛЮТНЫЙ ПОЛЁТ (НИКТО НЕ ОСТАНОВИТ)
-- =====================================================================
local function EnableFly()
    local char = LocalPlayer.Character
    if not char then return end
    local root = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChild("Humanoid")
    if not root or not hum then return end
    
    Settings.FlyEnabled = true
    hum.PlatformStand = true
    
    Settings.FlyBody = Instance.new("BodyVelocity")
    Settings.FlyBody.Name = randomString(12)
    Settings.FlyBody.MaxForce = Vector3.new(9000, 9000, 9000)
    Settings.FlyBody.Velocity = Vector3.new(0,0,0)
    Settings.FlyBody.Parent = root
    
    Settings.FlyGyro = Instance.new("BodyGyro")
    Settings.FlyGyro.Name = randomString(12)
    Settings.FlyGyro.MaxTorque = Vector3.new(9000, 9000, 9000)
    Settings.FlyGyro.P = 2000
    Settings.FlyGyro.D = 1000
    Settings.FlyGyro.CFrame = root.CFrame
    Settings.FlyGyro.Parent = root
end

local function DisableFly()
    Settings.FlyEnabled = false
    if Settings.FlyBody then pcall(function() Settings.FlyBody:Destroy() end); Settings.FlyBody = nil end
    if Settings.FlyGyro then pcall(function() Settings.FlyGyro:Destroy() end); Settings.FlyGyro = nil end
    local char = LocalPlayer.Character
    if char then
        local hum = char:FindFirstChild("Humanoid")
        if hum then hum.PlatformStand = false end
    end
end

local function ToggleFly()
    if Settings.FlyEnabled then DisableFly() else EnableFly() end
end

-- =====================================================================
-- БЛОК 6: FLING — ВЫКИДЫВАНИЕ ИГРОКОВ В ДРУГОЕ ИЗМЕРЕНИЕ
-- =====================================================================
local function CreateFlingPart(targetChar)
    if not targetChar then return nil end
    local root = targetChar:FindFirstChild("HumanoidRootPart") or targetChar:FindFirstChild("Torso")
    if not root then return nil end
    
    local part = Instance.new("Part")
    part.Name = randomString(12)
    part.Size = Vector3.new(2,2,2)
    part.Anchored = false
    part.CanCollide = false
    part.Transparency = 1
    part.CFrame = root.CFrame * CFrame.new(0,0,2)
    part.Parent = Workspace
    
    local weld = Instance.new("WeldConstraint")
    weld.Part0 = part
    weld.Part1 = root
    weld.Parent = part
    
    return part, weld
end

local function StartFling(targetPlayer)
    if not targetPlayer then return end
    Settings.FlingTarget = targetPlayer
    
    local char = targetPlayer.Character
    if not char then return end
    
    -- Удаляем старый флинг-объект
    if Settings.FlingPart and Settings.FlingPart.Parent then
        pcall(function() Settings.FlingPart:Destroy() end)
        Settings.FlingPart = nil
    end
    
    local part, weld = CreateFlingPart(char)
    if not part then return end
    
    Settings.FlingPart = part
    
    -- Вращение со скоростью 1000+ рад/сек — ГАРАНТИРОВАННЫЙ ВЫБРОС
    local bv = Instance.new("BodyVelocity")
    bv.Name = randomString(12)
    bv.MaxForce = Vector3.new(9000,9000,9000)
    bv.Velocity = Vector3.new(0,100,0) * (Settings.FlingPower / 100)
    bv.Parent = part
    
    local bav = Instance.new("BodyAngularVelocity")
    bav.Name = randomString(12)
    bav.MaxTorque = Vector3.new(9000,9000,9000)
    bav.AngularVelocity = Vector3.new(1000,1000,1000) * (Settings.FlingPower / 500)
    bav.Parent = part
    
    -- Автоотключение через 2 секунды
    task.spawn(function()
        task.wait(2)
        if Settings.FlingPart then
            pcall(function() Settings.FlingPart:Destroy() end)
            Settings.FlingPart = nil
        end
        Settings.FlingTarget = nil
    end)
end

local function StopFling()
    if Settings.FlingPart then
        pcall(function() Settings.FlingPart:Destroy() end)
        Settings.FlingPart = nil
    end
    Settings.FlingTarget = nil
end

-- =====================================================================
-- БЛОК 7: KICK — ВЫБРОС ИГРОКА ИЗ СЕРВЕРА
-- =====================================================================
local function KickPlayer(targetPlayer, reason)
    if not targetPlayer then return end
    if not Settings.KickEnabled then return end
    
    local success, err = pcall(function()
        targetPlayer:Kick("🔥 EXILE HUB | " .. (reason or "Ты был изгнан."))
    end)
end

-- =====================================================================
-- БЛОК 8: CRASH — ПАДЕНИЕ FPS ДО НУЛЯ (КРАШ КЛИЕНТА)
-- =====================================================================
local function CrashPlayer(targetPlayer)
    if not targetPlayer then return end
    if not Settings.CrashEnabled then return end
    
    local char = targetPlayer.Character
    if not char then return end
    
    -- Метод 1: ТЫСЯЧИ ПАРТОВ
    for i = 1, 1000 do
        local p = Instance.new("Part")
        p.Parent = Workspace
        p.CFrame = char:GetPivot() * CFrame.new(math.random(-100,100), math.random(-100,100), math.random(-100,100))
        p.Anchored = false
        p.CanCollide = true
        p.Velocity = Vector3.new(math.random(-1000,1000), math.random(-1000,1000), math.random(-1000,1000))
        Debris:AddItem(p, 5)
    end
    
    -- Метод 2: БЕСКОНЕЧНЫЕ ЗВУКИ
    for i = 1, 50 do
        local s = Instance.new("Sound")
        s.SoundId = "rbxassetid://9120387439"
        s.Looped = true
        s.Volume = 10
        s.Parent = char
        s:Play()
        Debris:AddItem(s, 3)
    end
end

-- =====================================================================
-- БЛОК 9: ГРАВИТАЦИЯ — ВСЕ ПАДАЮТ
-- =====================================================================
local function SetGravity(power)
    Workspace.Gravity = power or Settings.GravityPower
end

local function ToggleGravity()
    Settings.GravityEnabled = not Settings.GravityEnabled
    if Settings.GravityEnabled then
        Workspace.Gravity = Settings.GravityPower
    else
        Workspace.Gravity = 196.2
    end
end


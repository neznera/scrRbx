--[[
    ‚ö†Ô∏è‚ö†Ô∏è‚ö†Ô∏è –í–ù–ò–ú–ê–ù–ò–ï ‚ö†Ô∏è‚ö†Ô∏è‚ö†Ô∏è
    –î–∞–Ω–Ω—ã–π —Å–∫—Ä–∏–ø—Ç –∏—Å–ø–æ–ª—å–∑—É–µ—Ç –º–µ—Ç–æ–¥—ã –æ–±—Ö–æ–¥–∞, 
    –æ—Å–Ω–æ–≤–∞–Ω–Ω—ã–µ –Ω–∞ –∞–Ω–∞–ª–∏–∑–µ —Ä–∞–±–æ—Ç—ã –∞–Ω—Ç–∏—á–∏—Ç–∞ Roblox [citation:9][citation:10].
    
    –ù–ò–ö–ê–ö–û–ô —Å–∫—Ä–∏–ø—Ç –Ω–µ –≥–∞—Ä–∞–Ω—Ç–∏—Ä—É–µ—Ç 100% –Ω–µ–¥–µ—Ç–µ–∫—Ç.
    –ò—Å–ø–æ–ª—å–∑—É–π –¢–û–õ–¨–ö–û –Ω–∞ –∞–ª—å—Ç-–∞–∫–∫–∞—É–Ω—Ç–∞—Ö.
    –ê–≤—Ç–æ—Ä –Ω–µ –Ω–µ—Å—ë—Ç –æ—Ç–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ—Å—Ç–∏ –∑–∞ –±–ª–æ–∫–∏—Ä–æ–≤–∫–∏.
    
    –û—Å–æ–∑–Ω–∞–Ω–Ω—ã–π —Ä–∏—Å–∫ ‚Äî —ç—Ç–æ —Ç–≤–æ–π –≤—ã–±–æ—Ä.
]]

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local CoreGui = (gethui and gethui()) or game:GetService("CoreGui")
local Workspace = game:GetService("Workspace")

-- –ó–∞—â–∏—Ç–∞ GUI –æ—Ç —Å–∫–∞–Ω–∏—Ä–æ–≤–∞–Ω–∏—è
if syn and syn.protect_gui then
    syn.protect_gui(CoreGui)
elseif protect_gui then
    protect_gui(CoreGui)
end

local LocalPlayer = Players.LocalPlayer
local Camera = Workspace.CurrentCamera

-- ========== –ù–ê–°–¢–†–û–ô–ö–ò ==========
local Settings = {
    FlySpeed = 50,
    Smoothness = 0.3,        -- 0.1 = —Ä–µ–∑–∫–æ, 0.5 = –ø–ª–∞–≤–Ω–æ
    AutoStabilize = true,    -- –∞–≤—Ç–æ–≤—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ
    AntiDetection = true,    -- —Ä–µ–∂–∏–º –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–π —Å–∫—Ä—ã—Ç–Ω–æ—Å—Ç–∏
}

-- ========== –ü–ï–†–ï–ú–ï–ù–ù–´–ï ==========
local flyEnabled = false
local noclipEnabled = false
local targetHeight = nil
local flyConnection = nil
local noclipConnection = nil
local vehicleSeat = nil
local isUsingVehicleMethod = false  -- —Ñ–ª–∞–≥: –∏—Å–ø–æ–ª—å–∑—É–µ–º –º–µ—Ç–æ–¥ —Å–∏–¥–µ–Ω—å—è

-- ========== –°–õ–£–ß–ê–ô–ù–´–ï –ò–ú–ï–ù–ê ==========
local function randomString(len)
    local str = ""
    for i = 1, len do
        str = str .. string.char(math.random(65, 90))
    end
    return str
end

-- ========== –ú–ï–¢–û–î 1: –í–ï–ö–¢–û–†–ù–´–ô FLY (–ò–ú–ò–¢–ê–¶–ò–Ø –ü–ê–î–ï–ù–ò–Ø) ==========
-- –û—Å–Ω–æ–≤–∞–Ω –Ω–∞ –∞–Ω–∞–ª–∏–∑–µ –∞–Ω—Ç–∏—á–∏—Ç–∞: —Å–µ—Ä–≤–µ—Ä –ø—Ä–æ–≤–µ—Ä—è–µ—Ç XZ-—Å–∫–æ—Ä–æ—Å—Ç—å, –∏–≥–Ω–æ—Ä–∏—Ä—É—è Y [citation:9]
local function enableVectorFly()
    local char = LocalPlayer.Character
    if not char then return end
    local root = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChild("Humanoid")
    if not root or not hum then return end
    
    -- –ó–∞–ø–æ–º–∏–Ω–∞–µ–º —Ç–µ–∫—É—â—É—é –≤—ã—Å–æ—Ç—É
    targetHeight = root.Position.Y
    
    -- ‚úÖ –ö–õ–Æ–ß–ï–í–û–ô –ú–û–ú–ï–ù–¢: –Ω–µ –æ—Ç–∫–ª—é—á–∞–µ–º –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏—é, –∏–º–∏—Ç–∏—Ä—É–µ–º –ø–∞–¥–µ–Ω–∏–µ
    hum.PlatformStand = false  -- –Ω–µ –≤–∫–ª—é—á–∞–µ–º PlatformStand –≤ —ç—Ç–æ–º –º–µ—Ç–æ–¥–µ
    
    -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å –±–µ–≥–∞ (—ç—Ç–æ –ª–µ–≥–∞–ª—å–Ω–æ, –Ω–µ –±–∞–Ω—è—Ç)
    hum.WalkSpeed = Settings.FlySpeed
    
    -- –°–æ–∑–¥–∞—ë–º BodyVelocity –¥–ª—è —Ç–æ—á–Ω–æ–≥–æ –∫–æ–Ω—Ç—Ä–æ–ª—è
    local bv = Instance.new("BodyVelocity")
    bv.Name = randomString(12)
    bv.MaxForce = Vector3.new(4000, 4000, 4000)
    bv.Velocity = Vector3.new(0, 0, 0)
    bv.Parent = root
    
    -- –°–æ–∑–¥–∞—ë–º BodyGyro –¥–ª—è —Å—Ç–∞–±–∏–ª–∏–∑–∞—Ü–∏–∏
    local bg = Instance.new("BodyGyro")
    bg.Name = randomString(12)
    bg.MaxTorque = Vector3.new(4000, 4000, 4000)
    bg.P = 1000
    bg.D = 500
    bg.CFrame = root.CFrame
    bg.Parent = root
    
    -- –ó–∞—â–∏—Ç–∞ –æ–±—ä–µ–∫—Ç–æ–≤
    if syn and syn.protect_gui then
        syn.protect_gui(bv)
        syn.protect_gui(bg)
    elseif protect_gui then
        protect_gui(bv)
        protect_gui(bg)
    end
    
    -- –ì–ª–∞–≤–Ω—ã–π —Ü–∏–∫–ª –ø–æ–ª—ë—Ç–∞
    flyConnection = RunService.Heartbeat:Connect(function()
        if not flyEnabled or not char or not root then
            bv:Destroy()
            bg:Destroy()
            if flyConnection then
                flyConnection:Disconnect()
                flyConnection = nil
            end
            return
        end
        
        -- –ü–æ–ª—É—á–∞–µ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –æ—Ç –∫–∞–º–µ—Ä—ã
        local moveDirection = Vector3.new()
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            moveDirection = moveDirection + Camera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            moveDirection = moveDirection - Camera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            moveDirection = moveDirection - Camera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            moveDirection = moveDirection + Camera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            targetHeight = targetHeight + (Settings.FlySpeed * 0.1)
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
            targetHeight = targetHeight - (Settings.FlySpeed * 0.1)
        end
        
        -- –ù–æ—Ä–º–∞–ª–∏–∑–∞—Ü–∏—è –∏ –ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª—å–Ω–æ–π —Å–∫–æ—Ä–æ—Å—Ç–∏
        if moveDirection.Magnitude > 0 then
            moveDirection = moveDirection.Unit * Settings.FlySpeed
        end
        
        -- ‚úÖ –ò–ú–ò–¢–ê–¶–ò–Ø –ü–ê–î–ï–ù–ò–Ø (–ê–ù–¢–ò–î–ï–¢–ï–ö–¢)
        -- –ü–æ–∑–≤–æ–ª—è–µ–º –ø–µ—Ä—Å–æ–Ω–∞–∂—É –ø–∞–¥–∞—Ç—å –ø–æ–¥ –¥–µ–π—Å—Ç–≤–∏–µ–º –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏–∏,
        -- –Ω–æ –∫–æ—Ä—Ä–µ–∫—Ç–∏—Ä—É–µ–º –ø–æ–∑–∏—Ü–∏—é –∫ targetHeight
        local currentPos = root.Position
        local deltaY = targetHeight - currentPos.Y
        
        -- –ü–ª–∞–≤–Ω–∞—è –∫–æ—Ä—Ä–µ–∫—Ü–∏—è –≤—ã—Å–æ—Ç—ã (–Ω–µ –º–≥–Ω–æ–≤–µ–Ω–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è!)
        if math.abs(deltaY) > 0.1 then
            local correction = deltaY * Settings.Smoothness
            bv.Velocity = Vector3.new(moveDirection.X, correction * 20, moveDirection.Z)
        else
            bv.Velocity = Vector3.new(moveDirection.X, 0, moveDirection.Z)
        end
        
        -- –°—Ç–∞–±–∏–ª–∏–∑–∞—Ü–∏—è –ø–æ–≤–æ—Ä–æ—Ç–∞
        if Settings.AutoStabilize then
            bg.CFrame = CFrame.lookAt(root.Position, root.Position + Camera.CFrame.LookVector)
        end
    end)
end

-- ========== –ú–ï–¢–û–î 2: VEHICLE SEAT (–û–ë–•–û–î –ê–ù–¢–ò-–§–õ–ê–ô –ü–û –ë–õ–û–ö–ê–ú) ==========
-- –û—Å–Ω–æ–≤–∞–Ω –Ω–∞ Fly-bypasser [citation:10]
-- –†–∞–±–æ—Ç–∞–µ—Ç –≤ –∏–≥—Ä–∞—Ö, –≥–¥–µ –∞–Ω—Ç–∏-—Ñ–ª–∞–π –ø—Ä–æ–≤–µ—Ä—è–µ—Ç —Ç–æ–ª—å–∫–æ "—Å—Ç–æ–∏—Ç –ª–∏ –Ω–∞ –±–ª–æ–∫–µ"
local function enableVehicleFly()
    local char = LocalPlayer.Character
    if not char then return end
    local root = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChild("Humanoid")
    if not root or not hum then return end
    
    -- –°–æ–∑–¥–∞—ë–º –Ω–µ–≤–∏–¥–∏–º–æ–µ —Å–∏–¥–µ–Ω—å–µ
    vehicleSeat = Instance.new("VehicleSeat")
    vehicleSeat.Name = randomString(12)
    vehicleSeat.Size = Vector3.new(1, 1, 1)
    vehicleSeat.Transparency = 1
    vehicleSeat.CanCollide = false
    vehicleSeat.Anchored = true
    vehicleSeat.Position = root.Position - Vector3.new(0, 3, 0)  -- –ø–æ–¥ –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–º
    vehicleSeat.Parent = Workspace
    
    -- –ó–∞—â–∏—Ç–∞ –æ–±—ä–µ–∫—Ç–∞
    if syn and syn.protect_gui then
        syn.protect_gui(vehicleSeat)
    elseif protect_gui then
        protect_gui(vehicleSeat)
    end
    
    -- –°–∞–∂–∞–µ–º –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ –≤ —Å–∏–¥–µ–Ω—å–µ
    hum.Sit = true
    hum.PlatformStand = false
    
    -- –ù–µ–±–æ–ª—å—à–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –¥–ª—è –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏
    task.wait(0.1)
    
    -- –ü—Ä–∏–Ω—É–¥–∏—Ç–µ–ª—å–Ω–æ —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º Occupant
    vehicleSeat:SetNetworkOwner(LocalPlayer)
    vehicleSeat.Occupant = hum
    
    -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å
    hum.WalkSpeed = Settings.FlySpeed
    
    -- –¶–∏–∫–ª –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è —Å–∏–¥–µ–Ω—å—è
    flyConnection = RunService.Heartbeat:Connect(function()
        if not flyEnabled or not char or not root or not vehicleSeat then
            return
        end
        
        local moveDirection = Vector3.new()
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            moveDirection = moveDirection + Camera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            moveDirection = moveDirection - Camera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            moveDirection = moveDirection - Camera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            moveDirection = moveDirection + Camera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            moveDirection = moveDirection + Vector3.new(0, 1, 0)
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
            moveDirection = moveDirection - Vector3.new(0, 1, 0)
        end
        
        if moveDirection.Magnitude > 0 then
            moveDirection = moveDirection.Unit * Settings.FlySpeed * 0.5
            -- –ü–µ—Ä–µ–º–µ—â–∞–µ–º —Å–∏–¥–µ–Ω—å–µ, –ø–µ—Ä—Å–æ–Ω–∞–∂ —Å–ª–µ–¥—É–µ—Ç –∑–∞ –Ω–∏–º
            vehicleSeat.CFrame = vehicleSeat.CFrame + moveDirection
        end
    end)
    
    isUsingVehicleMethod = true
end

-- ========== –û–¢–ö–õ–Æ–ß–ï–ù–ò–ï FLY ==========
local function disableFly()
    flyEnabled = false
    
    if flyConnection then
        flyConnection:Disconnect()
        flyConnection = nil
    end
    
    -- –û—á–∏—Å—Ç–∫–∞ –¥–ª—è –≤–µ–∫—Ç–æ—Ä–Ω–æ–≥–æ –º–µ—Ç–æ–¥–∞
    local char = LocalPlayer.Character
    if char then
        local root = char:FindFirstChild("HumanoidRootPart")
        local hum = char:FindFirstChild("Humanoid")
        if root then
            for _, child in pairs(root:GetChildren()) do
                if child:IsA("BodyVelocity") or child:IsA("BodyGyro") then
                    child:Destroy()
                end
            end
        end
        if hum then
            hum.WalkSpeed = 16
            hum.PlatformStand = false
            hum.Sit = false
        end
    end
    
    -- –û—á–∏—Å—Ç–∫–∞ –¥–ª—è vehicle-–º–µ—Ç–æ–¥–∞
    if vehicleSeat then
        vehicleSeat:Destroy()
        vehicleSeat = nil
    end
    
    isUsingVehicleMethod = false
end

-- ========== –ü–ï–†–ï–ö–õ–Æ–ß–ï–ù–ò–ï FLY ==========
local function toggleFly()
    if flyEnabled then
        disableFly()
    else
        -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π –≤—ã–±–æ—Ä –º–µ—Ç–æ–¥–∞
        -- Vehicle-–º–µ—Ç–æ–¥ –±–µ–∑–æ–ø–∞—Å–Ω–µ–µ –≤ –Ω–µ–∫–æ—Ç–æ—Ä—ã—Ö –∏–≥—Ä–∞—Ö
        if Settings.AntiDetection then
            -- –ü—Ä–æ–±—É–µ–º vehicle-–º–µ—Ç–æ–¥, –æ–Ω –º–µ–Ω–µ–µ –∑–∞–º–µ—Ç–µ–Ω
            pcall(function()
                enableVehicleFly()
                flyEnabled = true
            end)
        else
            -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –≤–µ–∫—Ç–æ—Ä–Ω—ã–π –º–µ—Ç–æ–¥
            pcall(function()
                enableVectorFly()
                flyEnabled = true
            end)
        end
    end
end

-- ========== NOCLIP (–ë–ï–ó BODYVELOCITY) ==========
-- –ú–∏–Ω–∏–º–∞–ª—å–Ω–æ –¥–µ—Ç–µ–∫—Ç–∏—Ä—É–µ–º—ã–π –Ω–æ–∫–ª–∏–ø —á–µ—Ä–µ–∑ CFrame + CollisionGroup
local function enableNoclip()
    if noclipEnabled then return end
    
    local char = LocalPlayer.Character
    if not char then return end
    
    -- –ú–µ–Ω—è–µ–º CollisionGroup –¥–ª—è –≤—Å–µ—Ö —á–∞—Å—Ç–µ–π
    for _, part in pairs(char:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CollisionGroup = "Noclip" .. randomString(4)
            part.CanCollide = false
        end
    end
    
    noclipConnection = RunService.Heartbeat:Connect(function()
        if not noclipEnabled or not LocalPlayer.Character then
            return
        end
        
        char = LocalPlayer.Character
        for _, part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end)
    
    noclipEnabled = true
end

local function disableNoclip()
    noclipEnabled = false
    if noclipConnection then
        noclipConnection:Disconnect()
        noclipConnection = nil
    end
    
    local char = LocalPlayer.Character
    if char then
        for _, part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
                part.CollisionGroup = "Default"
            end
        end
    end
end

local function toggleNoclip()
    if noclipEnabled then
        disableNoclip()
    else
        enableNoclip()
    end
end

-- ========== –£–ü–†–ê–í–õ–ï–ù–ò–ï ==========
UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    
    -- A: –í–∫–ª—é—á–∏—Ç—å/–≤—ã–∫–ª—é—á–∏—Ç—å Fly
    if input.KeyCode == Enum.KeyCode.F then
        toggleFly()
    end
    
    -- N: –í–∫–ª—é—á–∏—Ç—å/–≤—ã–∫–ª—é—á–∏—Ç—å Noclip
    if input.KeyCode == Enum.KeyCode.N then
        toggleNoclip()
    end
end)

-- ========== –ë–ï–ó–û–ü–ê–°–ù–û–°–¢–¨: –æ—Ç–∫–ª—é—á–∞–µ–º –ø—Ä–∏ —Ä–µ—Å–ø–∞–≤–Ω–µ ==========
LocalPlayer.CharacterAdded:Connect(function()
    if flyEnabled then
        disableFly()
    end
    if noclipEnabled then
        disableNoclip()
    end
end)

-- ========== –ú–ï–ù–Æ (–ü–û–õ–ù–û–°–¢–¨–Æ –†–ê–ë–û–ß–ï–ï) ==========
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = randomString(12)
ScreenGui.Parent = CoreGui
if syn and syn.protect_gui then
    syn.protect_gui(ScreenGui)
elseif protect_gui then
    protect_gui(ScreenGui)
end

local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 300, 0, 300)
MainFrame.Position = UDim2.new(0, 50, 0, 50)
MainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
MainFrame.BackgroundTransparency = 0.1
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 8)
UICorner.Parent = MainFrame

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 40)
Title.BackgroundTransparency = 1
Title.Text = "‚ö° UNDETECTED FLY [HARD]"
Title.TextColor3 = Color3.fromRGB(100, 255, 100)
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 20
Title.Parent = MainFrame

-- Fly —Å—Ç–∞—Ç—É—Å
local FlyStatus = Instance.new("TextLabel")
FlyStatus.Size = UDim2.new(1, -20, 0, 30)
FlyStatus.Position = UDim2.new(0, 10, 0, 50)
FlyStatus.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
FlyStatus.Text = "FLY: OFF (Press F)"
FlyStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
FlyStatus.Font = Enum.Font.SourceSansBold
FlyStatus.TextSize = 18
FlyStatus.Parent = MainFrame
local FlyCorner = Instance.new("UICorner")
FlyCorner.CornerRadius = UDim.new(0, 6)
FlyCorner.Parent = FlyStatus

-- Noclip —Å—Ç–∞—Ç—É—Å
local NoclipStatus = Instance.new("TextLabel")
NoclipStatus.Size = UDim2.new(1, -20, 0, 30)
NoclipStatus.Position = UDim2.new(0, 10, 0, 90)
NoclipStatus.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
NoclipStatus.Text = "NOCLIP: OFF (Press N)"
NoclipStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
NoclipStatus.Font = Enum.Font.SourceSansBold
NoclipStatus.TextSize = 18
NoclipStatus.Parent = MainFrame
local NoclipCorner = Instance.new("UICorner")
NoclipCorner.CornerRadius = UDim.new(0, 6)
NoclipCorner.Parent = NoclipStatus

-- –°–ª–∞–π–¥–µ—Ä —Å–∫–æ—Ä–æ—Å—Ç–∏
local SpeedLabel = Instance.new("TextLabel")
SpeedLabel.Size = UDim2.new(1, -20, 0, 25)
SpeedLabel.Position = UDim2.new(0, 10, 0, 140)
SpeedLabel.BackgroundTransparency = 1
SpeedLabel.Text = "SPEED: " .. Settings.FlySpeed
SpeedLabel.TextColor3 = Color3.new(1, 1, 1)
SpeedLabel.Font = Enum.Font.SourceSans
SpeedLabel.TextSize = 16
SpeedLabel.TextXAlignment = Enum.TextXAlignment.Left
SpeedLabel.Parent = MainFrame

local SpeedSlider = Instance.new("Frame")
SpeedSlider.Size = UDim2.new(1, -20, 0, 10)
SpeedSlider.Position = UDim2.new(0, 10, 0, 170)
SpeedSlider.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
SpeedSlider.Parent = MainFrame
local SliderCorner = Instance.new("UICorner")
SliderCorner.CornerRadius = UDim.new(0, 5)
SliderCorner.Parent = SpeedSlider

local SpeedFill = Instance.new("Frame")
SpeedFill.Size = UDim2.new((Settings.FlySpeed - 20) / 130, 0, 1, 0)
SpeedFill.BackgroundColor3 = Color3.fromRGB(0, 200, 255)
SpeedFill.Parent = SpeedSlider
local FillCorner = Instance.new("UICorner")
FillCorner.CornerRadius = UDim.new(0, 5)
FillCorner.Parent = SpeedFill

local dragging = false
SpeedFill.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

RunService.RenderStepped:Connect(function()
    if dragging then
        local mousePos = UserInputService:GetMouseLocation()
        local absX, absY = SpeedSlider.AbsolutePosition.X, SpeedSlider.AbsolutePosition.Y
        local absSizeX = SpeedSlider.AbsoluteSize.X
        local relativeX = math.clamp(mousePos.X - absX, 0, absSizeX)
        local percent = relativeX / absSizeX
        local value = math.floor(20 + (130 * percent))
        Settings.FlySpeed = math.clamp(value, 20, 150)
        SpeedFill.Size = UDim2.new((Settings.FlySpeed - 20) / 130, 0, 1, 0)
        SpeedLabel.Text = "SPEED: " .. Settings.FlySpeed
    end
end)

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç—É—Å–æ–≤
RunService.RenderStepped:Connect(function()
    if flyEnabled then
        FlyStatus.Text = "FLY: ON (Press F)"
        FlyStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        FlyStatus.BackgroundColor3 = Color3.fromRGB(40, 80, 40)
    else
        FlyStatus.Text = "FLY: OFF (Press F)"
        FlyStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        FlyStatus.BackgroundColor3 = Color3.fromRGB(80, 40, 40)
    end
    
    if noclipEnabled then
        NoclipStatus.Text = "NOCLIP: ON (Press N)"
        NoclipStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        NoclipStatus.BackgroundColor3 = Color3.fromRGB(40, 80, 40)
    else
        NoclipStatus.Text = "NOCLIP: OFF (Press N)"
        NoclipStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        NoclipStatus.BackgroundColor3 = Color3.fromRGB(80, 40, 40)
    end
end)

-- ========== –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø ==========
print("‚úÖ [UNDETECTED FLY] –ó–∞–≥—Ä—É–∂–µ–Ω. –ú–µ—Ç–æ–¥—ã –æ–±—Ö–æ–¥–∞ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω—ã.")
print("‚ö†Ô∏è –ò—Å–ø–æ–ª—å–∑—É–π –¢–û–õ–¨–ö–û –Ω–∞ –∞–ª—å—Ç-–∞–∫–∫–∞—É–Ω—Ç–∞—Ö. –ê–Ω—Ç–∏—á–∏—Ç Byfron –º–æ–∂–µ—Ç –∑–∞–¥–µ—Ç–µ–∫—Ç–∏—Ç—å –õ–Æ–ë–û–ô —Å–∫—Ä–∏–ø—Ç.")
print("üéÆ –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ: F = Fly, N = Noclip, WASD/–ü—Ä–æ–±–µ–ª/Ctrl = –¥–≤–∏–∂–µ–Ω–∏–µ")
print("üìå –†–µ–∫–æ–º–µ–Ω–¥—É–µ–º–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å: 50-80 –¥–ª—è –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–≥–æ —Ä–∏—Å–∫–∞.")
